alias g='git'
compdef g=git
alias gst='git status'
compdef _git gst=git-status
alias gd='git diff'
compdef _git gd=git-diff
alias gtree='git log --graph --decorate --all --date-order'
compdef _git gtree=git-log
alias gcv='git commit -v'
compdef _git gcv=git-commit
alias grb='git rebase'
compdef _git grb=git-rebase
alias grbi='git rebase --interactive'
compdef _git grb=git-rebase
alias gm='git merge'
compdef _git gm=git-merge
alias gb='git branch'
compdef _git gb=git-branch
alias gba='git branch -la'
compdef _git gba=git-branch
alias gco='git checkout'
compdef _git gco=git-checkout

#dogegit
alias wow='git status'
compdef _git wow=git-status
alias very-push='git push'
compdef _git very-push=git-push
alias much-add='git add'
compdef _git much-add=git-add
alias so-rebase='git rebase'
compdef _git so-rebase=git-rebase
alias such-merge='git merge'
compdef _git such-merge=git-merge

#myfuncs
compdef _git merge-base-diff=git-merge
